{
	"name": "astro",
	"type": "module",
	"version": "0.0.1",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"dev:fast": "CACHE_ONLY=true astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"build:fast": "astro build",
		"build:fresh": "CACHE_BYPASS=true npm run build",
		"test": "vitest",
		"preview": "astro preview",
		"astro": "astro",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check --write",
		"format:check": "biome format --check",
		"lint:check": "biome lint --check",
		"check:check": "biome check --check",
		"format:src": "biome format --write src",
		"lint:src": "biome lint --write src",
		"check:src": "biome check --write src",
		"format:check:src": "biome format --check src",
		"lint:check:src": "biome lint --check src",
		"check:check:src": "biome check --check src",
		"cache:stats": "node scripts/cache-stats.js",
		"cache:clear": "rm -rf .astro/cache"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.6",
		"@astrojs/mdx": "^4.3.13",
		"@astrojs/rss": "^4.0.15",
		"@astrojs/tailwind": "^6.0.2",
		"@astrojs/vue": "^5.1.4",
		"astro": "^5.17.3",
		"astro-mermaid": "^1.2.0",
		"markdown-it": "^14.1.0",
		"mermaid": "^11.12.2",
		"sanitize-html": "^2.17.0",
		"sharp": "^0.33.5",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.9.3",
		"vue": "^3.5.26"
	},
	"devDependencies": {
		"@tailwindcss/typography": "^0.5.19",
		"@types/markdown-it": "^14.1.2",
		"@types/node": "^22.19.3",
		"@types/sanitize-html": "^2.16.0",
		"prettier": "^3.7.4",
		"prettier-plugin-astro": "^0.14.1",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	}
}
